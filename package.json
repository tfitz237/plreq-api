{
  "name": "project",
  "version": "1.2.1",
  "description": "plreq",
  "author": "",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:watch": "nest build --watch --webpack",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.10.0",
    "@nestjs/core": "^6.10.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.10.1",
    "@nestjs/platform-socket.io": "^6.10.1",
    "@nestjs/typeorm": "^6.2.0",
    "@nestjs/websockets": "^6.10.1",
    "cheerio": "^1.0.0-rc.3",
    "cron": "^1.7.2",
    "deepmerge": "^4.2.2",
    "fs-extra": "^8.1.0",
    "helmet": "^3.21.2",
    "jdownloader-api": "^1.1.0",
    "mkdirp": "^0.5.1",
    "parse-torrent-name": "^0.5.4",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "password-hash": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.3",
    "sqlite": "^3.0.3",
    "typeorm": "^0.2.20"
  },
  "devDependencies": {
    "@nestjs/testing": "^6.10.1",
    "@types/cheerio": "^0.22.15",
    "@types/cron": "^1.7.1",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.0.23",
    "@types/node": "^12.12.11",
    "@types/socket.io": "^2.1.4",
    "@types/supertest": "^2.0.8",
    "jest": "^24.9.0",
    "prettier": "^1.19.1",
    "supertest": "^3.1.0",
    "ts-jest": "^24.1.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.5.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "5.20.1",
    "typescript": "^3.7.2",
    "webpack": "^4.41.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
